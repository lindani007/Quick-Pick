<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QuickPick.QuikApp.Pages.Employer.ProductAdding"
            xmlns:charts="clr-namespace:Microcharts.Maui;assembly=Microcharts.Maui"
             Title="Aisles And Products" BackgroundColor="White">
    <ContentPage Title="Add Aisles" BackgroundColor="WhiteSmoke">
        <ScrollView>
            <StackLayout Spacing="10" Padding="30">
                <Label Text=" Add Your Aisle" FontSize="30" FontAttributes="Bold" 
                       HorizontalOptions="Center" >
                    <Label.Shadow>
                        <Shadow Brush="Black"
                            Opacity="0.5"
                            Radius="10"
                            Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <Label Text="Add your aisles to make it easy for your customeres to find product fast." 
                       FontSize="20" FontAttributes="Bold" HorizontalOptions="Center">
                    <Label.Shadow>
                        <Shadow Brush="Black"
                            Opacity="0.5"
                            Radius="10"
                            Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <HorizontalStackLayout Spacing="25" Padding="15">
                    <Border BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                Opacity="0.5"
                                Radius="10"
                                Offset="10,10" />
                        </Border.Shadow>
                        <Grid>
                            <StackLayout Spacing="10" Padding="10">
                                <Label Text="Add an image to make your aisle more visual" FontSize="30"/>
                                <Border StrokeThickness="0">
                                    <Border.StrokeShape>
                                        <RoundRectangle CornerRadius="20"/>
                                    </Border.StrokeShape>
                                    <Image Source="empty.webp" HeightRequest="400" Aspect="AspectFill"/>
                                </Border>
                            </StackLayout>
                        </Grid>
                    </Border>
                    <Grid VerticalOptions="Center">
                        <StackLayout Spacing="30" Padding="10">
                            <Border BackgroundColor="White">
                                <Border.StrokeShape>
                                    <RoundRectangle CornerRadius="25"/>
                                </Border.StrokeShape>
                                <Border.Shadow>
                                    <Shadow Brush="Black"
                                            Opacity="0.5"
                                            Radius="10"
                                            Offset="10,10" />
                                </Border.Shadow>
                                <StackLayout WidthRequest="500" Spacing="10" Padding="10">
                                    <Label Text="Aisle Name" FontSize="30" />
                                    <Border WidthRequest="450" HorizontalOptions="Center">
                                        <Border.StrokeShape>
                                            <RoundRectangle CornerRadius="20"/>
                                        </Border.StrokeShape>
                                        <Entry Placeholder="Aisle Name" HorizontalTextAlignment="Center"
                                               x:Name="aisleNameEntry" WidthRequest="450" FontSize="30"/>
                                    </Border>
                                </StackLayout>
                            </Border>
                            <Border BackgroundColor="White">
                                <Border.StrokeShape>
                                    <RoundRectangle CornerRadius="25"/>
                                </Border.StrokeShape>
                                <Border.Shadow>
                                    <Shadow Brush="Black"
                                            Opacity="0.5"
                                            Radius="10"
                                            Offset="10,10" />
                                </Border.Shadow>
                                <StackLayout WidthRequest="500" Spacing="10" Padding="10">
                                    <Label Text="Description" FontSize="30" />
                                    <Border WidthRequest="450" HorizontalOptions="Center">
                                        <Border.StrokeShape>
                                            <RoundRectangle CornerRadius="20"/>
                                        </Border.StrokeShape>
                                        <Entry Placeholder="Aisle Description" HorizontalTextAlignment="Center"
                                                 x:Name="aisleDescriptionEntry" WidthRequest="450" FontSize="30"/>
                                    </Border>
                                </StackLayout>
                            </Border>
                        </StackLayout>
                    </Grid>
                </HorizontalStackLayout>
                <HorizontalStackLayout Spacing="15" HorizontalOptions="Center">
                    <Border WidthRequest="400">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                    Opacity="0.5"
                                    Radius="10"
                                    Offset="10,10" />
                        </Border.Shadow>
                        <Button TextColor="White" BackgroundColor="#66BB6A" Text="Add Aisle" FontAttributes="Bold" FontSize="30" WidthRequest="400" HorizontalOptions="Center"
                                Clicked="AddAisleClicked"/>
                    </Border>
                    <Border WidthRequest="400">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                    Opacity="0.5"
                                    Radius="10"
                                    Offset="10,10" />
                        </Border.Shadow>
                        <Button TextColor="White" BackgroundColor="#66BB6A" Text="Done" FontAttributes="Bold" FontSize="30" WidthRequest="400" HorizontalOptions="Center"
                        Clicked="SaveAisleClicked"/>
                    </Border>
                </HorizontalStackLayout>
            </StackLayout>
        </ScrollView>
    </ContentPage>
    <ContentPage Title="Add Items" BackgroundColor="WhiteSmoke">
        <ScrollView>
            <StackLayout Spacing="10" Padding="30">
                <Label Text=" Add Your Items" FontSize="30" FontAttributes="Bold" 
                    HorizontalOptions="Center" >
                    <Label.Shadow>
                        <Shadow Brush="Black"
                            Opacity="0.5"
                            Radius="10"
                            Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <Label Text="Add your items so that your custormers can skip the queue and grab there goods in minutes." 
                    FontSize="20" FontAttributes="Bold" HorizontalOptions="Center">
                    <Label.Shadow>
                        <Shadow Brush="Black"
                            Opacity="0.5"
                            Radius="10"
                            Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <HorizontalStackLayout Spacing="25" Padding="15">
                    <Border BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                Opacity="0.5"
                                Radius="10"
                                Offset="10,10" />
                        </Border.Shadow>
                        <Grid>
                            <StackLayout Spacing="10" Padding="10">
                                <Label Text="Add an image to make your Item more visual" FontSize="30"/>
                                <Image Source="empty.webp" HeightRequest="400" Aspect="AspectFill"/>
                            </StackLayout>
                        </Grid>
                    </Border>
                    <StackLayout Spacing="15" Padding="10">
                        <Border BackgroundColor="White">
                            <Border.StrokeShape>
                                <RoundRectangle CornerRadius="25"/>
                            </Border.StrokeShape>
                            <Border.Shadow>
                                <Shadow Brush="Black"
                                    Opacity="0.5"
                                    Radius="10"
                                    Offset="10,10" />
                            </Border.Shadow>
                            <StackLayout WidthRequest="500" Spacing="10" Padding="10">
                                <Label Text="Item Name" FontSize="30" />
                                <Border WidthRequest="450" HorizontalOptions="Center">
                                    <Border.StrokeShape>
                                        <RoundRectangle CornerRadius="20"/>
                                    </Border.StrokeShape>
                                    <Entry Placeholder="Item Name" HorizontalTextAlignment="Center"
                                    x:Name="itemNameEntry" WidthRequest="450" FontSize="30"/>
                                </Border>
                            </StackLayout>
                        </Border>
                        <Border IsVisible="False" x:Name="itesmCollection" BackgroundColor="White">
                            <Border.StrokeShape>
                                <RoundRectangle CornerRadius="25"/>
                            </Border.StrokeShape>
                            <Border.Shadow>
                                <Shadow Brush="Black"
                                     Opacity="0.5"
                                     Radius="10"
                                     Offset="10,10" />
                            </Border.Shadow>
                            <Grid HeightRequest="100">
                                <CollectionView>
                                    <CollectionView.ItemTemplate>
                                        <DataTemplate>
                                            <StackLayout Spacing="10">
                                                <Label x:Name="lblItemName" FontSize="20"/>
                                            </StackLayout>
                                        </DataTemplate>
                                    </CollectionView.ItemTemplate>
                                </CollectionView>
                            </Grid>
                        </Border>
                        <Border BackgroundColor="White">
                            <Border.StrokeShape>
                                <RoundRectangle CornerRadius="25"/>
                            </Border.StrokeShape>
                            <Border.Shadow>
                                <Shadow Brush="Black"
                                    Opacity="0.5"
                                    Radius="10"
                                    Offset="10,10" />
                            </Border.Shadow>
                            <StackLayout WidthRequest="500" Spacing="10" Padding="10">
                                <Label Text="Description" FontSize="30" />
                                <Border WidthRequest="450" HorizontalOptions="Center">
                                    <Border.StrokeShape>
                                        <RoundRectangle CornerRadius="20"/>
                                    </Border.StrokeShape>
                                    <Entry Placeholder="Item Description" HorizontalTextAlignment="Center"
                                x:Name="itemDescriptionEntry" WidthRequest="450" FontSize="30"/>
                                </Border>
                            </StackLayout>
                        </Border>
                        <Border BackgroundColor="White">
                            <Border.StrokeShape>
                                <RoundRectangle CornerRadius="25"/>
                            </Border.StrokeShape>
                            <Border.Shadow>
                                <Shadow Brush="Black"
                                    Opacity="0.5"
                                    Radius="10"
                                    Offset="10,10" />
                            </Border.Shadow>
                            <StackLayout WidthRequest="500" Spacing="10" Padding="10">
                                <Label Text="Price" FontSize="30" />
                                <Border WidthRequest="450" HorizontalOptions="Center">
                                    <Border.StrokeShape>
                                        <RoundRectangle CornerRadius="20"/>
                                    </Border.StrokeShape>
                                    <Entry Placeholder="Item Price" HorizontalTextAlignment="Center"
                                   x:Name="itemPriceEntry" WidthRequest="450" FontSize="30"/>
                                </Border>
                            </StackLayout>
                        </Border>
                        <Border BackgroundColor="White">
                            <Border.StrokeShape>
                                <RoundRectangle CornerRadius="25"/>
                            </Border.StrokeShape>
                            <Border.Shadow>
                                <Shadow Brush="Black"
                                        Opacity="0.5"
                                        Radius="10"
                                        Offset="10,10" />
                            </Border.Shadow>
                            <StackLayout WidthRequest="500" Spacing="10" Padding="10">
                                <Label Text="Quantity" FontSize="30" />
                                <Border WidthRequest="450" HorizontalOptions="Center">
                                    <Border.StrokeShape>
                                        <RoundRectangle CornerRadius="20"/>
                                    </Border.StrokeShape>
                                    <Entry Placeholder="Item Quantity" HorizontalTextAlignment="Center"
                                       x:Name="itemQuantityEntry" WidthRequest="450" FontSize="30"/>
                                </Border>
                            </StackLayout>
                        </Border>
                    </StackLayout>
                </HorizontalStackLayout>
                <HorizontalStackLayout Spacing="15" HorizontalOptions="Center">
                    <Border WidthRequest="400">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                 Opacity="0.5"
                                 Radius="10"
                                 Offset="10,10" />
                        </Border.Shadow>
                        <Button TextColor="White" BackgroundColor="#66BB6A" Text="Add Aisle" FontAttributes="Bold" FontSize="30" WidthRequest="400" HorizontalOptions="Center"
                             Clicked="AddAisleClicked"/>
                    </Border>
                    <Border WidthRequest="400">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                 Opacity="0.5"
                                 Radius="10"
                                 Offset="10,10" />
                        </Border.Shadow>
                        <Button TextColor="White" BackgroundColor="#66BB6A" Text="Save Aisles" FontAttributes="Bold" FontSize="30" WidthRequest="400" HorizontalOptions="Center"
                     Clicked="SaveAisleClicked"/>
                    </Border>
                </HorizontalStackLayout>
            </StackLayout>
        </ScrollView>
    </ContentPage>
    <ContentPage Title="Choose Aisle" BackgroundColor="WhiteSmoke">
        <ScrollView>
            <StackLayout Spacing="20" Padding="15">
                <Label Text="Aisles" FontSize="40" FontAttributes="Bold" HorizontalOptions="Center">
                    <Label.Shadow>
                        <Shadow Brush="Black"
                        Opacity="0.5"
                        Radius="10"
                        Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <Border WidthRequest="450" HorizontalOptions="Center" BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="20"/>
                        </Border.StrokeShape>
                    <Border.Shadow>
                        <Shadow Brush="Black"
                                Opacity="0.5"
                                Radius="10"
                                Offset="10,10"/>
                    </Border.Shadow>
                        <Entry Placeholder="Search Aisle" FontSize="30" HorizontalOptions="Center"
                               WidthRequest="450" HorizontalTextAlignment="Center" x:Name="aisleSearchEntry"/>
                </Border>
                <Label Text="Select An Aisle You Want To Add It Items" FontSize="30" HorizontalOptions="Center">
                    <Label.Shadow>
                        <Shadow Brush="Black"
             Opacity="0.5"
             Radius="10"
             Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <CollectionView>
                    <CollectionView.ItemTemplate>
                        <DataTemplate>
                            <Border BackgroundColor="White">
                                <Border.StrokeShape>
                                    <RoundRectangle CornerRadius="25"/>
                                </Border.StrokeShape>
                                <Border.Shadow>
                                    <Shadow Brush="Black"
                                        Opacity="0.5"
                                        Radius="10"
                                        Offset="10,10" />
                                </Border.Shadow>
                                <StackLayout Spacing="10" Padding="15" Orientation="Horizontal">
                                    <Border StrokeThickness="0">
                                        <Border.StrokeShape>
                                            <RoundRectangle CornerRadius="20"/>
                                        </Border.StrokeShape>
                                        <Image Source="income.png" HeightRequest="200" Aspect="AspectFill"/>
                                    </Border>
                                    <StackLayout VerticalOptions="Center" WidthRequest="100">
                                        <Label Text="Drinks" FontSize="24" FontAttributes="Bold"/>
                                        <Label Text="Refreshments" FontSize="18"/>
                                    </StackLayout>
                                </StackLayout>
                            </Border>
                        </DataTemplate>
                    </CollectionView.ItemTemplate>
                </CollectionView>
                <HorizontalStackLayout HorizontalOptions="Center" Spacing="50" Padding="20">
                    <Border BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
            Opacity="0.5"
            Radius="10"
            Offset="10,10" />
                        </Border.Shadow>
                        <StackLayout Spacing="10" Padding="15" Orientation="Horizontal">
                            <Border StrokeThickness="0">
                                <Border.StrokeShape>
                                    <RoundRectangle CornerRadius="20"/>
                                </Border.StrokeShape>
                                <Image Source="income.png" HeightRequest="200" Aspect="AspectFill"/>
                            </Border>
                            <StackLayout VerticalOptions="Center" WidthRequest="100">
                                <Label Text="Drinks" FontSize="24" FontAttributes="Bold"/>
                                <Label Text="Refreshments And Cold Drinks" FontSize="18"/>
                            </StackLayout>
                        </StackLayout>
                    </Border>
                    <Border BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
            Opacity="0.5"
            Radius="10"
            Offset="10,10" />
                        </Border.Shadow>
                        <StackLayout Spacing="10" Padding="15" Orientation="Horizontal">
                            <Border StrokeThickness="0">
                                <Border.StrokeShape>
                                    <RoundRectangle CornerRadius="20"/>
                                </Border.StrokeShape>
                                <Image Source="income.png" HeightRequest="200" Aspect="AspectFill"/>
                            </Border>
                            <StackLayout VerticalOptions="Center">
                                <Label Text="Drinks" FontSize="24" FontAttributes="Bold"/>
                                <Label Text="Refreshments" FontSize="18"/>
                            </StackLayout>
                        </StackLayout>
                    </Border>
                    <Border BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
            Opacity="0.5"
            Radius="10"
            Offset="10,10" />
                        </Border.Shadow>
                        <StackLayout Spacing="10" Padding="15" Orientation="Horizontal">
                            <Border StrokeThickness="0">
                                <Border.StrokeShape>
                                    <RoundRectangle CornerRadius="20"/>
                                </Border.StrokeShape>
                                <Image Source="income.png" HeightRequest="200" Aspect="AspectFill"/>
                            </Border>
                            <StackLayout VerticalOptions="Center">
                                <Label Text="Drinks" FontSize="24" FontAttributes="Bold"/>
                                <Label Text="Refreshments" FontSize="18"/>
                            </StackLayout>
                        </StackLayout>
                    </Border>
                </HorizontalStackLayout>
            </StackLayout>
        </ScrollView>
    </ContentPage>
    <ContentPage Title="Sales Analysis" BackgroundColor="WhiteSmoke">
        <ContentPage.Shadow>
            <Shadow Brush="Black"
                 Opacity="0.5"
                 Radius="10"
                 Offset="10,10" />
        </ContentPage.Shadow>
        <ScrollView>
            <StackLayout Spacing="15" Padding="10">
                <Label Text="Your Invertory Analysis" FontSize="30" FontAttributes="Bold" HorizontalOptions="Center" Margin="0,40,0,20">
                    <Label.Shadow>
                        <Shadow Brush="Black"
                         Opacity="0.5"
                         Radius="10"
                         Offset="10,10" />
                    </Label.Shadow>
                </Label>
                <HorizontalStackLayout Spacing="20" HorizontalOptions="Center">
                    <Border BackgroundColor="White" WidthRequest="300">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="15"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                 Opacity="0.5"
                                 Radius="10"
                                 Offset="10,10" />
                        </Border.Shadow>
                        <StackLayout Spacing="10" WidthRequest="100" HeightRequest="100"  HorizontalOptions="Center">
                            <Label Text="Daily Income" FontSize="20" FontAttributes="Bold"  HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
                            <Label Text="R0,00" FontSize="15" x:Name="totalCost" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
                        </StackLayout>
                    </Border>
                    <Border BackgroundColor="White" WidthRequest="300">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="15"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                             Opacity="0.5"
                             Radius="10"
                             Offset="10,10" />
                        </Border.Shadow>
                        <StackLayout Spacing="10" WidthRequest="100" HeightRequest="100" HorizontalOptions="Center">
                            <Label Text="Weekly Income" FontSize="20" FontAttributes="Bold" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
                            <Label Text=" R0,00" FontSize="15" x:Name="totalIncome" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
                        </StackLayout>
                    </Border>
                    <Border BackgroundColor="White" WidthRequest="300">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="15"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                             Opacity="0.5"
                             Radius="10"
                             Offset="10,10" />
                        </Border.Shadow>
                        <StackLayout Spacing="10" WidthRequest="100" HeightRequest="100" HorizontalOptions="Center">
                            <Label Text="Mothly Income" FontSize="20" FontAttributes="Bold" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
                            <Label Text="R0,00" FontSize="15" x:Name="totalProfi" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
                        </StackLayout>
                    </Border>
                </HorizontalStackLayout>
                <HorizontalStackLayout Spacing="20" Padding="15" HorizontalOptions="Center">
                    <StackLayout Spacing="15"   Padding="15">
                        <Border BackgroundColor="White">
                            <Border.StrokeShape>
                                <RoundRectangle CornerRadius="25"/>
                            </Border.StrokeShape>
                            <Border.Shadow>
                                <Shadow Brush="Black"
                             Opacity="0.5"
                             Radius="10"
                             Offset="10,10" />
                            </Border.Shadow>
                            <Grid>
                                <ScrollView>
                                    <StackLayout Spacing="10" Padding="15" WidthRequest="365">
                                        <Label Text="Items You Added" FontAttributes="Bold" FontSize="Header"/>
                                        <ScrollView >
                                            <StackLayout>
                                                <Label x:Name="lblEmpty" Text="You haven't added any items yet" FontSize="30" HorizontalOptions="Center"/>
                                                <Label IsVisible="False" x:Name="lblInstructions" Text="Select Item to View it In Details" FontSize="30" HorizontalOptions="Center"/>
                                                <CollectionView>
                                                    <CollectionView.ItemTemplate>
                                                        <DataTemplate>
                                                            <ScrollView>
                                                                <Border BackgroundColor="White">
                                                                    <Border.StrokeShape>
                                                                        <RoundRectangle CornerRadius="25"/>
                                                                    </Border.StrokeShape>
                                                                    <Border.Shadow>
                                                                        <Shadow Brush="Black"
                                                                             Opacity="0.5"
                                                                             Radius="10"
                                                                             Offset="10,10" />
                                                                    </Border.Shadow>
                                                                    <StackLayout Orientation="Horizontal">
                                                                        <Label x:Name="lblItemName" FontSize="20"/>
                                                                        <Label x:Name="lblItemQuantity" FontSize="20"/>
                                                                        <Label x:Name="lblItemDate" FontSize="20"/>
                                                                    </StackLayout>
                                                                </Border>
                                                            </ScrollView>
                                                        </DataTemplate>
                                                    </CollectionView.ItemTemplate>
                                                </CollectionView>
                                            </StackLayout>
                                        </ScrollView>
                                    </StackLayout>
                                </ScrollView>
                            </Grid>
                        </Border>
                    </StackLayout >
                    <Border BackgroundColor="White">
                        <Border.StrokeShape>
                            <RoundRectangle CornerRadius="25"/>
                        </Border.StrokeShape>
                        <Border.Shadow>
                            <Shadow Brush="Black"
                                 Opacity="0.5"
                                 Radius="10"
                                 Offset="10,10" />
                        </Border.Shadow>
                        <Grid>
                            <StackLayout Spacing="10" Padding="15" BackgroundColor="White">
                                <HorizontalStackLayout Spacing="20">
                                    <Picker Title="Display Income As : " WidthRequest="300" HorizontalOptions="Start">
                                        <Picker.Items>
                                            <x:String>Pie Chart</x:String>
                                            <x:String>Bar Char</x:String>
                                            <x:String>Line Chart</x:String>
                                            <x:String>Half Radial Gauge Chart</x:String>
                                        </Picker.Items>
                                    </Picker>
                                    <Picker Title="View Income For : " WidthRequest="300" HorizontalOptions="Start">
                                        <Picker.Items>
                                            <x:String>January</x:String>
                                            <x:String>February</x:String>
                                            <x:String>March</x:String>
                                            <x:String>April</x:String>
                                            <x:String>May</x:String>
                                            <x:String>June</x:String>
                                            <x:String>July</x:String>
                                            <x:String>August</x:String>
                                            <x:String>September</x:String>
                                            <x:String>October</x:String>
                                            <x:String>November</x:String>
                                            <x:String>December</x:String>
                                        </Picker.Items>
                                    </Picker>
                                </HorizontalStackLayout>
                                <Label Text="Income Chart" FontAttributes="Bold" FontSize="Header"/>
                                <ScrollView Orientation="Horizontal">
                                    <StackLayout>
                                        <charts:ChartView x:Name="incomeChartView" HeightRequest="700" WidthRequest="900"/>
                                    </StackLayout>
                                </ScrollView>
                            </StackLayout>
                        </Grid>
                    </Border>
                </HorizontalStackLayout>
            </StackLayout>
        </ScrollView>
    </ContentPage>
</TabbedPage>